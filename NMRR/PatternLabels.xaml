<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- PRBS Section -->
    <sys:String x:Key="PRBS_Duration">10</sys:String>
    <sys:String x:Key="PRBS_Amplitude">2</sys:String>
    <sys:String x:Key="PRBS_SR">20</sys:String>
    <sys:String x:Key="PRBS_MaxWidth">100</sys:String>
    <sys:String x:Key="PRBS_Cycles">1</sys:String>

    <!-- Pulse Section -->
    <sys:String x:Key="Pulse_Amplitude">2</sys:String>
    <sys:String x:Key="Pulse_Numbers">1</sys:String>
    <sys:String x:Key="Pulse_Width">100</sys:String>
    <sys:String x:Key="Pulse_Period">300</sys:String>
    <sys:String x:Key="Pulse_InitDelay">200</sys:String>
    <sys:Boolean x:Key="PRBS_Include">True</sys:Boolean>

    <!-- TV Section -->
    <sys:String x:Key="TV_T12">1</sys:String>
    <sys:String x:Key="TV_T34">2</sys:String>
    <sys:String x:Key="TV_T56">1</sys:String>
    <sys:String x:Key="TV_A12">0</sys:String>
    <sys:String x:Key="TV_A34">20</sys:String>
    <sys:String x:Key="TV_A56">0</sys:String>
    <sys:String x:Key="TV_V23">10</sys:String>
    <sys:String x:Key="TV_V45">10</sys:String>
    <sys:String x:Key="TV_Reps">1</sys:String>
</ResourceDictionary>
